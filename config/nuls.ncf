#出块间隔时间(单位：s)
packingInterval=10
#共识委托抵押资产链ID
agentChainId=2021
#共识委托抵押资产ID
agentAssetId=1
#共识奖励资产ID(共识奖励必须为本链资产)
awardAssetId=1
#共识交易手续费单价
feeUnit=100000
#总通胀量
totalInflationAmount=30000000000000000
#初始通胀金额
inflationAmount=4000000000000000
#通胀开始计算时间(单位:S)
initTime=TODO
#通缩比例(如果没有通缩则设为100)
