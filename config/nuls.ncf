#通缩间隔时间(单位：S) 4Y
deflationTimeInterval=126144000
#创建节点保证金最小值
depositMin=300000000000
#创建节点保证金最大值
depositMax=1000000000000
#节点出块最少委托金额
commissionMin=1000000000000
#节点最大委托金额
commissionMax=100000000000000
#单笔委托最低金额
entrusterDepositMin=10000000000

[smart-contract]
#合约视图方法调用最大消耗的Gas
maxViewGas=100000000

[public-service]
#public-service模块对外的rpc端口号
rpcPort=8003
#数据库url地址
